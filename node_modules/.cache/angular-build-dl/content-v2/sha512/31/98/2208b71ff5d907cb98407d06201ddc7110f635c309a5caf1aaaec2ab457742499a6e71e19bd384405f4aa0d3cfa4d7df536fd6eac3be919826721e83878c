    let AuthGuard = class AuthGuard {
      constructor(myRoute, apiService) {
        this.myRoute = myRoute;
        this.apiService = apiService;
      }

      canActivate(next, state) {
        if (this.apiService.isLoggedIn()) {
          return true;
        } else {
          this.myRoute.navigateByUrl("login");
          return false;
        }
      }

    };

