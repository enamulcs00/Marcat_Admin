    let VendorSalesReportComponent = class VendorSalesReportComponent {
      constructor(router, apiService, commonService) {
        this.router = router;
        this.apiService = apiService;
        this.commonService = commonService;
        this.flagUserList = false;
        this.page = 1;
        this.search = '';
        this.filterBy = '';
        this.filterList = [{
          viewValue: 'New',
          value: 'New'
        }, {
          viewValue: 'Accepted',
          value: 'Accepted'
        }, {
          viewValue: 'Cancelled',
          value: 'Canceled'
        }, {
          viewValue: 'Rejected',
          value: 'Rejected'
        }, {
