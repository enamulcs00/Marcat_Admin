        }));
      }

      submit() {
        this.submitted = true;

        if (this.submitted && this.updateProfileForm.valid) {
          const data = new FormData();

          if (this.picUploader === true) {
            data.append('profilePic', this.imageFile, this.imageFile.name);
          }

          data.append('firstName', this.updateProfileForm.get('firstName').value), data.append('lastName', this.updateProfileForm.get('lastName').value), data.append('email', this.profileData.email), data.append('address', this.updateProfileForm.get('address').value), data.append('countryCode', this.updateProfileForm.get('countryCode').value), data.append('phone', this.updateProfileForm.get('phone').value), data.append('deliveryType', this.updateProfileForm.get('deliveryType').value), this.apiService.updateProfile(data).subscribe(res => {
