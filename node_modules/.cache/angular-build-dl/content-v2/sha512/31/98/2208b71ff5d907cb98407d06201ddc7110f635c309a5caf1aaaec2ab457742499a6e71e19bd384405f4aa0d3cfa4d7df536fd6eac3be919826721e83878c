    let EndorsementProductComponent = class EndorsementProductComponent {
      constructor(router, apiService, route, sanitizer, serverUrl, commonService) {
        this.router = router;
        this.apiService = apiService;
        this.route = route;
        this.sanitizer = sanitizer;
        this.serverUrl = serverUrl;
        this.commonService = commonService;
        this.length = 100;
        this.pageSize = 5;
        this.pageSizeOptions = [5, 10, 25, 100];
        this.filterBy = 'active';
        this.search = '';
        this.categoryId = '';
        this.subCategory = '';
        this.flagData = false;
        this.page = 1;
        this.flagUserList = false;
        this.isApproved = true;
        this.isEndorse = true;
        this.flag = false;
        this.flagSearch = true;
        this.sub = this.route.queryParams.subscribe(params => {
          // Defaults to 0 if no query param provided.
          this.id = params['id'];
        });
        this.user = JSON.parse(sessionStorage.getItem('Markat_User'));
        console.log(this.user);
        this.sellerId = this.id; //alert(this.id)
      }

      ngOnInit() {
        // alert(this.id)
        this.imageUrl = this.serverUrl.imageUrl;
        this.getAllProducts();
      }

      filterSelected(e) {
        if (this.filterBy) {
          this.flag = true;
        } else {
          this.flag = false;
        }

        console.log(e.value);
        this.filterBy = e.value;
        this.getAllProducts();
      }

      approvedSelected(e) {
        if (this.isApproved) {
          this.flagapproval = true;
        } else {
          this.flagapproval = false;
        }

        console.log(e.value);
        this.isApproved = e.value;
        this.getAllProducts();
      }

      getAllProducts() {
        //Method if isApproved is selected for some value
        this.progress = true;
        this.apiService.getProductsForEndorsement(this.page, this.pageSize, this.filterBy, this.isApproved, this.search, this.sellerId, this.isEndorse).subscribe(res => {
          if (res.success) {
            this.progress = false;

            if (res.data.length > 0) {
              this.flagData = false;
              this.productList = res.data;
              console.log(res.data);
              this.length = res.total; // this.productList = res.data
            } else {
              this.flagData = true;
            }
          } else {
            this.progress = false;
            this.commonService.errorToast(res.message);
            this.flagData = true;
          }
        });
      }

      searchMethod() {
        this.flagSearch = false;
        this.getAllProducts();
      }

      clearSearch() {
        this.flagSearch = true;
        this.search = '';
        this.getAllProducts();
      }

      vendorProductListAfterPageSizeChanged(e) {
        if (e.pageIndex == 0) {
          this.page = 1;
          this.pageSize = e.pageSize; // this.page = e.pageIndex;
          //  this.srNo = e.pageIndex * e.pageSize

          this.flagUserList = false;
        } else {
          if (e.previousPageIndex < e.pageIndex) {
            this.page = e.pageIndex + 1;
            this.srNo = e.pageIndex * e.pageSize;
            this.flagUserList = true;
          } else {
            this.page = e.pageIndex;
            this.srNo = e.pageIndex * e.pageSize;
            this.flagUserList = true;
          }
        }

        this.getAllProducts();
      }

      edndorsementProduct(id) {
        sweetalert2__WEBPACK_IMPORTED_MODULE_2___default.a.fire({
          title: "Are you sure?",
          text: "What will be the Commission percentage for the Vendor",
          icon: "info",
          showCancelButton: true,
          input: 'number',
          confirmButtonColor: "#3085D6",
          cancelButtonColor: "#d33",
          confirmButtonText: "Yes",
          cancelButtonText: "Cancel",
          allowOutsideClick: true
        }).then(result => {
          if (result.isConfirmed) {
            if (result.value) {
              let body = {
                product: id,
                commission: result.value
              };
              this.progress = true;
              this.apiService.endorsementRequest(body).subscribe(res => {
                console.log(res);

                if (res.success) {
                  this.progress = false;
                  console.log('requestSent');
                  this.commonService.successToast(res.message);
                } else {
                  this.progress = false;
                  this.commonService.errorToast(res.message);
                }
              });
            } else {
              this.commonService.errorToast('Please add commission');
            }
          } else {
            console.log("nothing changed");
          }
        });
      }

      goToaddProduct() {
        this.router.navigate(['/addproduct']);
      }

      goToeditProduct(id) {
        this.router.navigate(['/editProduct'], {
          queryParams: {
            "id": id
          }
        });
      }

      goToviewProduct(id) {
        this.router.navigate(['/viewProduct'], {
          queryParams: {
            "id": id
          }
        });
      }

      goToVendorList() {
        this.router.navigate(['venderManagement']);
      }

      back() {
        window.history.back();
      }

    };

