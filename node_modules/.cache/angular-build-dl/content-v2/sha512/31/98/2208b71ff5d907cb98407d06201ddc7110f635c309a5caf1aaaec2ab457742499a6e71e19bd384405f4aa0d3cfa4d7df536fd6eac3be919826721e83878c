        } else {
          this.getAllCategory();
          this.sellerId = this.userDetails._id; //  this.setradio('product');

          this.addDiscountForm.get('dicountOn').setValue('product');
        }

        this.addDiscountForm.get('bannerImage').disable();
      }

      ngAfterViewChecked() {
