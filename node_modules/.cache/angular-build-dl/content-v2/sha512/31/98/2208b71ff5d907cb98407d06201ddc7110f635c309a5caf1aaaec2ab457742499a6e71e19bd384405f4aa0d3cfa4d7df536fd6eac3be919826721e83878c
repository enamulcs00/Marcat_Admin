      constructor(router, apiService, urlService, commonService) {
        this.router = router;
        this.apiService = apiService;
        this.urlService = urlService;
        this.commonService = commonService;
        this.layoutstyle = [{
          viewValue: 'Grid',
          value: 1
        }, {
          viewValue: 'List',
          value: 2
        }, {
          viewValue: 'Card',
          value: 3
        }];
        this.user = JSON.parse(this.apiService.getUser());
