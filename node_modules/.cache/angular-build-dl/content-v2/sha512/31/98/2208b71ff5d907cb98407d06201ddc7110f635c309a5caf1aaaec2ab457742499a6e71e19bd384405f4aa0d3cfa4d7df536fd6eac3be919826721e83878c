          queryParams: {
            "id": id,
            "name": name
          }
        });
      }

      goToreportGraph(id) {
        this.router.navigate(['reportGraph'], {
          queryParams: {
            id: id
          }
        });
      }

      back() {
        window.history.back();
      }

